@import "../../wxParse/wxParse.wxss";
page{
  background: #f9f9f9
}
.top-bar{
  width: 100%;
  height: 80rpx;
  background-color: #fff;
  position: fixed;
  top: 0rpx;
  border-top: 1rpx solid #f3f3f3
}
.active-line{
  width: 80rpx;
  height: 4rpx;
  margin: auto;
  background: #fe702e
}
.yy-item{
  width: 100%;
  min-height: 200rpx;
  background-color: #fff;
  box-shadow: 0px 0px 5px 0px 
		rgba(135, 135, 135, 0.2);
}
.pa-th{
  padding: 0 40rpx 
}
.border-bd{
  border-bottom: 1px dotted #e1e1e1
}
.cri{
  width: 40rpx;
  height: 40rpx;
  border-radius: 40rpx;
  background-color: #f9f9f9;
  margin-top: -20rpx
}
.cri:first-child{
  margin-left: -20rpx
}
.cri:last-child{
  margin-right: -20rpx
}
.iohs{
  width: 230rpx;
	height: 140rpx;
}
.sada{
  width: 320rpx;
  height: 100rpx
}
.buybtn{
  width: 140rpx;
	height: 50rpx;
	background-color: #fe702e;
	border-radius: 8rpx;
	border: solid 1px #fe702e;
  color: #fff;
  font-size: 24rpx;
  line-height: 50rpx;
  text-align: center
}
.buybtn2{
  width: 140rpx;
	height: 50rpx;
	background-color: #fff;
	border-radius: 8rpx;
	border: solid 1px #323232;
  color: #000;
